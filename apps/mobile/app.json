{
  "expo": {
    "scheme": "com.strive.app",
    "name": "Strive",
    "slug": "Strive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/ios/icon-adaptive-light.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/icons/ios/icon-dark.png",
        "light": "./assets/icons/ios/icon-light.png"
      },
      "bundleIdentifier": "com.anonymous.Strive",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android/icon-adaptive-dark.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.Strive"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/ios/icon-adaptive-light.png",
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "imageWidth": 200
        }
      ],
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Strive to access your camera"
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8428fd4-cb6d-47d9-97d1-7698077026d6"
      }
    }
  }
}
